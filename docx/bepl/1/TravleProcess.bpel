<bpel:process name="TravleProcess"
         targetNamespace="http://www.ruc.edu.cn/bpel/sample"
         suppressJoinFailure="yes"
         xmlns:tns="http://www.ruc.edu.cn/bpel/sample"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns="http://impl.service.iir.ruc.edu.cn">

